{
  "name": "drq-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "build:dev": "cross-env NODE_ENV=development next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "setup": "node ./scripts/setup-repo.js",
    "deploy": "node ./scripts/deploy.js",
    "deploy:prod": "cross-env DEPLOY_ENV=production node ./scripts/deploy.js",
    "deploy:dev": "cross-env DEPLOY_ENV=development node ./scripts/deploy.js",
    "build:branches": "node ./scripts/auto-build.js",
    "build:branches:win": "./scripts/auto-build.bat",
    "build:branches:unix": "./scripts/auto-build.sh",
    "manage:branches": "node ./scripts/manage-branches.js",
    "manage:branches:win": "./scripts/manage-branches.bat",
    "manage:branches:unix": "./scripts/manage-branches.sh",
    "cleanup": "rimraf .next out",
    "cache:clear": "rimraf .next/cache",
    "cache:warm": "node ./scripts/warm-cache.js",
    "monitoring:notify": "node ./scripts/monitoring-notify.js",
    "db:backup": "node ./scripts/db-backup.js"
  },
  "dependencies": {
    "@types/node": "20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "next": "14.0.3",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
